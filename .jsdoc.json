{
  "opts": {
    "destination": "./doc/",
    "encoding": "utf8",
    "readme": "README.md",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "plugins": [],
  "source": {
    "include": ["src"],
    "includePattern": "\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  }
}
